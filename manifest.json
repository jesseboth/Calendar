{
    "name": "My custom new tab page",
    "description": "Overrides the new tab page",
    "version": "1.0",
    "incognito": "split",
    "chrome_url_overrides": {
      "newtab": "tab/custom_tab.html"
    },
    "permissions": [
      "webRequest",
      "webRequestBlocking"
    ],
    "manifest_version": 2
}